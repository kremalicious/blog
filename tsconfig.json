{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ],
    "jsxImportSource": "react"
  },
  "exclude": ["node_modules", "public", "dist", "*.js"],
  "include": ["./*.ts", "./src/**/*", "./scripts/*.ts", "./.jest/**/*"]
}
