    "nord-visual-studio-code": "github:arcticicestudio/nord-visual-studio-code",
    "pigeon-maps": "^0.21.3",
    "pigeon-marker": "^0.3.4",
    "react": "^18.2.0",
    "react-clipboard.js": "^2.0.16",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "rehype-react": "^7.2.0",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "slugify": "^1.6.6",
    "unified": "^11.0.2",
    "use-debounce": "^9.0.4",
    "viem": "^1.9.0",
    "wagmi": "^1.3.10"
